{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)\\.( js|css|png|jpg|jpeg|webp|avif|svg|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=604800, stale-while-revalidate=2592000"
        }
      ]
    }
  ]
}
