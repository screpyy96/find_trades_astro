{
  "headers": [
    {
      "source": "/(.*)\\.( js|css|png|jpg|jpeg|webp|avif|svg|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=604800, stale-while-revalidate=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/studio/:path*",
      "destination": "https://7094dn36.api.sanity.io/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/cere-oferta",
